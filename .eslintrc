{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "camelcase": [2, {"properties": "never"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "curly": [2],
    "dot-notation": [2],
    "eqeqeq": [2],
    "indent": [2, 2, {"SwitchCase": 1}],
    "jsx-quotes": [2, "prefer-double"],
    "linebreak-style": [2, "unix"],
    "max-len": [2, {"code": 100}],
    "no-alert": [0],
    "no-cond-assign": [2],
    "no-console": [0],
    "no-var": [2],
    "no-empty": [2],
    "no-extra-semi": [2],
    "no-multiple-empty-lines": [2, {"max": 2, "maxEOF":1}],
    "no-multi-spaces": [2],
    "no-const-assign": [2],
    "no-trailing-spaces": [2],
    "no-unused-vars": [2, {"varsIgnorePattern": "^_", "args": "none"}],
    "no-throw-literal": [2],
    "object-curly-spacing": [2, "never"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "strict": [0, "never"]
  },
  "globals": {
    "ga": true
  }
}
